{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepare": "install-peers",
    "start": "NODE_ENV=production node dist/index.js",
    "start:dev": "nodemon src/index.ts  --inspect=0.0.0.0:9229 ",
    "start:dev:brk": "nodemon src/index.ts  --inspect-brk=0.0.0.0:9229",
    "build": "mv tsconfig.json tsconfig.backup.json && cp tsconfig.prod.json tsconfig.json && tsc -p . && mv tsconfig.backup.json tsconfig.json",
    "lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{js,ts,tsx}' --fix",
    "lint:fix-dry": "eslint 'src/**/*.{js,ts,tsx}' --fix-dry-run",
    "husky": {
      "hooks": {
        "pre-commit": "lint-staged"
      }
    },
    "lint-staged": {
      "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
        "prettier --write",
        "npm run lint:fix"
      ]
    },
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
